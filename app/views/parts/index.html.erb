<h1>Listing parts</h1>
<h2> total <%= @parts.count %> </h2>

<table>
  <tr>
    <th>Content</th>
    <th>Source</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= raw part.headline %></td>
    <td><%= part.source_id %></td>
    <td><%= link_to 'Show', part %></td>
    <td><%= link_to 'Edit', edit_part_path(part) %></td>
    <td><%= link_to 'Destroy', part, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Part', new_part_path %>
<%= link_to 'Upload Parts', upload_parts_path %>
